package com.shiva;

public class Person {
	private int pid;
	private String pname;
	private int ppoints;
	public int getPid() {
		return pid;
	}
	public void setPid(int pid) {
		this.pid = pid;
	}
	public String getPname() {
		return pname;
	}
	public void setPname(String pname) {
		this.pname = pname;
	}
	public int getPpoints() {
		return ppoints;
	}
	public void setPpoints(int ppoints) {
		this.ppoints = ppoints;
	}
}
